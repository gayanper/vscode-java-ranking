{
  "name": "vscode-java-ranking",
  "displayName": "vscode-java-ranking-vscode",
  "description": "VisualStudio Code vscode-java-ranking Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "license": "EPL-2.0",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./dist/extension.js",
  "extensionDependencies": [
    "redhat.java"
  ],
  "contributes": {
    "javaExtensions": [
      "./jars/org.gap.vscode.javaranking.ext.jar"
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build-server": "./node_modules/.bin/gulp build-server"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2",
    "vscode-languageserver": "8.1.0",
    "gulp": "4.0.2",
    "gulp-rename": "2.0.0",
    "fsevents": "2.3.2",
    "fs-extra": "11.1.1"
  }
}